<?xml version="1.0" encoding="utf-8"?>
<resources>


  <style name="DCloudTheme" parent="@style/DeviceDefault.Light">
    <item name="android:windowFullscreen">true</item>
    <item name="android:colorEdgeEffect">#333333</item>
    <!--<item name="android:windowIsTranslucent">true</item>-->
    <item name="android:statusBarColor">@color/statusBarColor</item>
  </style>

  <style name="DeviceDefault.Light" parent="QMUI.Compat">
    <item name="android:windowNoTitle">true</item>
    <item name="android:colorEdgeEffect">#333333</item>
    <item name="android:windowBackground">@color/ime_background</item>
  </style>

  <style name="DeviceDefault" parent="@android:style/Theme.DeviceDefault">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:colorEdgeEffect">#333333</item>
    <item name="android:windowBackground">@color/ime_background</item>
  </style>

  <style name="TranslucentTheme" parent="QMUI.Compat">
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">true</item>
    <item name="android:colorEdgeEffect">#333333</item>
    <item name="android:windowBackground">@null</item>
  </style>
</resources>